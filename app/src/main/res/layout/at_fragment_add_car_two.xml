<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="165px"
            android:paddingLeft="45px">

            <TextView
                android:layout_width="279px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/at_car_owner"
                android:textColor="@color/_666666"
                android:textSize="42px" />

            <EditText
                android:id="@+id/et_car_owner"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="279px"
                android:layout_marginRight="45px"
                android:background="@null"
                android:hint="@string/at_hint_car_owner"
                android:textColor="@color/_333333"
                android:textColorHint="@color/_DDDDDD"
                android:textSize="45px" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3px"
                android:layout_alignParentBottom="true"
                android:background="@color/_F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="165px"
            android:paddingLeft="45px">

            <TextView
                android:layout_width="279px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/at_id_number1"
                android:textColor="@color/_666666"
                android:textSize="42px" />

            <EditText
                android:id="@+id/et_id_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="279px"
                android:layout_marginRight="45px"
                android:background="@null"
                android:hint="@string/at_input_id_number"
                android:textColor="@color/_333333"
                android:textColorHint="@color/_DDDDDD"
                android:textSize="45px" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3px"
                android:layout_alignParentBottom="true"
                android:background="@color/_F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="165px"
            android:paddingLeft="45px">

            <TextView
                android:layout_width="279px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/at_phone3"
                android:textColor="@color/_666666"
                android:textSize="42px" />

            <EditText
                android:id="@+id/et_phone_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="279px"
                android:layout_marginRight="45px"
                android:background="@null"
                android:hint="@string/at_text_import_phonenumber"
                android:maxLength="11"
                android:textColor="@color/_333333"
                android:textColorHint="@color/_DDDDDD"
                android:textSize="45px" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3px"
                android:layout_alignParentBottom="true"
                android:background="@color/_F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="165px"
            android:paddingLeft="45px">

            <TextView
                android:layout_width="279px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/at_plate_number"
                android:textColor="@color/_666666"
                android:textSize="42px" />

            <RelativeLayout
                android:id="@+id/rl_plate_numbers"
                android:layout_width="204px"
                android:layout_height="90px"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="45px"
                android:background="@drawable/at_shape_3px_3px_cccccc"
                android:gravity="center_vertical">

                <TextView
                    android:id="@+id/tv_plate_number"
                    android:layout_width="120px"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="ç²¤"
                    android:textColor="@color/_333333"
                    android:textSize="45px" />

                <ImageView
                    android:id="@+id/img_plate_numbers_arrow"
                    android:layout_width="84px"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@color/_EBB080"
                    android:scaleType="center"
                    android:src="@drawable/atonline_info_triangle" />
            </RelativeLayout>

            <TextView
                android:id="@+id/tv_plate_numbers"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="279px"
                android:layout_marginRight="210px"
                android:gravity="center_vertical"
                android:maxLength="10"
                android:text="@string/at_input_plate_number"
                android:textColor="@color/_333333"
                android:textSize="45px" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3px"
                android:layout_alignParentBottom="true"
                android:background="@color/_F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_car_park"
            android:layout_width="match_parent"
            android:layout_height="165px"
            android:paddingLeft="45px">

            <TextView
                android:layout_width="279px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/at_car_park"
                android:textColor="@color/_666666"
                android:textSize="42px" />

            <TextView
                android:id="@+id/tv_car_park"
                android:layout_width="369px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="279px"
                android:textColor="@color/_333333"
                android:textSize="45px" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="45px"
                android:src="@drawable/atico_junp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3px"
                android:layout_alignParentBottom="true"
                android:background="@color/_F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_car_building"
            android:layout_width="match_parent"
            android:layout_height="165px"
            android:paddingLeft="45px">

            <TextView
                android:layout_width="279px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/at_car_building"
                android:textColor="@color/_666666"
                android:textSize="42px" />

            <TextView
                android:id="@+id/tv_car_building"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="279px"
                android:layout_marginRight="45px"
                android:maxLines="2"
                android:textColor="@color/_333333"
                android:textSize="45px" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="45px"
                android:src="@drawable/atico_junp"
                android:visibility="gone" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3px"
                android:layout_alignParentBottom="true"
                android:background="@color/_F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="165px"
            android:paddingLeft="45px">

            <TextView
                android:layout_width="279px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/at_brand"
                android:textColor="@color/_666666"
                android:textSize="42px" />

            <EditText
                android:id="@+id/et_car_brand"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="279px"
                android:layout_marginRight="45px"
                android:background="@null"
                android:hint="@string/at_hint_car_brand"
                android:textColor="@color/_333333"
                android:textColorHint="@color/_DDDDDD"
                android:textSize="45px" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3px"
                android:layout_alignParentBottom="true"
                android:background="@color/_F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="165px"
            android:paddingLeft="45px">

            <TextView
                android:layout_width="279px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/at_car_color"
                android:textColor="@color/_666666"
                android:textSize="42px" />

            <EditText
                android:id="@+id/et_car_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="279px"
                android:layout_marginRight="45px"
                android:background="@null"
                android:hint="@string/at_hint_car_color"
                android:textColor="@color/_333333"
                android:textColorHint="@color/_DDDDDD"
                android:textSize="45px" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3px"
                android:layout_alignParentBottom="true"
                android:background="@color/_F0F0F0" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="165px"
            android:paddingLeft="45px">

            <TextView
                android:layout_width="279px"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="@string/at_car_type"
                android:textColor="@color/_666666"
                android:textSize="42px" />

            <EditText
                android:id="@+id/et_car_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="279px"
                android:layout_marginRight="45px"
                android:background="@null"
                android:hint="@string/at_hint_car_type"
                android:textColor="@color/_333333"
                android:textColorHint="@color/_DDDDDD"
                android:textSize="45px" />

            <View
                android:layout_width="match_parent"
                android:layout_height="3px"
                android:layout_alignParentBottom="true"
                android:background="@color/_F0F0F0" />
        </RelativeLayout>
    </LinearLayout>
</ScrollView>