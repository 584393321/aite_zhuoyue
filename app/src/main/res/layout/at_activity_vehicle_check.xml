<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <com.aliyun.ayland.widget.titlebar.ATMyTitleBar
            android:id="@+id/titlebar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:bartext="@string/at_vehicle_check" />

        <View
            android:layout_width="match_parent"
            android:layout_height="3px"
            android:background="@color/_EEEEEE" />

        <com.scwang.smartrefresh.layout.SmartRefreshLayout
            android:id="@+id/smartRefreshLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:srlAccentColor="@android:color/white"
            app:srlPrimaryColor="#444444">

            <com.scwang.smartrefresh.layout.header.ClassicsHeader
                android:id="@+id/srl_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <com.aliyun.ayland.widget.ATObservableScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_marginTop="80px"
                        android:layout_height="750px">

                        <com.aliyun.ayland.widget.ATSemicircleProgressView
                            android:id="@+id/semicircleProgressView"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:semicircleSize="684px"
                            app:semicirclebackgroundLineColor="@color/_C7B8A4"
                            app:semicirclebackgroundlineSize="18px"
                            app:semicirclefrontLineColor="@color/_FFC156"
                            app:semicirclelineSize="24px" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:background="@drawable/at_shape_circle_516pxffffff_4pxfce5c6"
                            android:elevation="10px"
                            android:gravity="center_horizontal"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="120px"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/tv_current"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/_F79327"
                                    android:textSize="90px" />

                                <TextView
                                    android:id="@+id/tv_all"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/_666666"
                                    android:textSize="42px" />
                            </LinearLayout>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="6px"
                                android:text="@string/at_current_empty_vehicle"
                                android:textColor="@color/_666666"
                                android:textSize="42px" />
                        </LinearLayout>
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/tv_park_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="42px"
                        android:textColor="@color/_333333"
                        android:textSize="60px" />

                    <TextView
                        android:id="@+id/tv_park_code"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="42px"
                        android:layout_marginTop="24px"
                        android:textColor="@color/_999999"
                        android:textSize="36px" />

                    <LinearLayout
                        android:layout_width="996px"
                        android:layout_height="605px"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="43px"
                        android:background="@drawable/at_shape_12px_e6fff8f0"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:paddingLeft="50px"
                            android:paddingRight="70px">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="@string/at_title_vehicle"
                                android:textColor="@color/_333333"
                                android:textSize="45px" />

                            <TextView
                                android:id="@+id/tv_title_space"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:textColor="@color/_333333"
                                android:textSize="45px" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:paddingLeft="50px"
                            android:paddingRight="70px">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="@string/at_current_empty_vehicle1"
                                android:textColor="@color/_333333"
                                android:textSize="45px" />

                            <TextView
                                android:id="@+id/tv_empty_vehicle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:textColor="@color/_333333"
                                android:textSize="45px" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:paddingLeft="50px"
                            android:paddingRight="70px">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="@string/at_buy_out_vehicle"
                                android:textColor="@color/_333333"
                                android:textSize="45px" />

                            <TextView
                                android:id="@+id/tv_buy_out_space"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:textColor="@color/_333333"
                                android:textSize="45px" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:paddingLeft="50px"
                            android:paddingRight="70px">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:text="@string/at_public_vehicle"
                                android:textColor="@color/_333333"
                                android:textSize="45px" />

                            <TextView
                                android:id="@+id/tv_public_space"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:textColor="@color/_333333"
                                android:textSize="45px" />
                        </RelativeLayout>
                    </LinearLayout>
                </LinearLayout>
            </com.aliyun.ayland.widget.ATObservableScrollView>
        </com.scwang.smartrefresh.layout.SmartRefreshLayout>
    </LinearLayout>